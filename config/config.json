{
  "bot": {
    "token": "${DISCORD_TOKEN}"
  },
  "channels": {
    "join_log": "${JOIN_LOG_CHANNEL_ID}",
    "leave_log": "${LEAVE_LOG_CHANNEL_ID}"
  },
  "fallback_messages": {
    "join": "Welcome, {user}! Enjoy your stay in {server.name}.",
    "leave": "**{username}** **disconnected.** their presence.. deleted."
  },
  "events": {
    "guildMemberAdd": {
      "embed": {
        "title": "{eye_emoji} {bot_name} is watching.",
        "description": "**{username}** logged in.\ntyping...\nbut will you ever press send?",
        "eye_emoji_id": "${EYE_EMOJI_ID}",
        "author_icon_url": "${AUTHOR_ICON_URL}"
      }
    }
  },
  "guild_id": "${GUILD_ID}",
  "permissions": {
    "allowed_role_ids": [
      "${ALLOWED_ROLE_ID}"
    ]
  },
  "bot_presence": {
    "status": "${BOT_STATUS}",
    "activity": {
      "type": "${BOT_ACTIVITY_TYPE}",
      "name": "${BOT_ACTIVITY_NAME}",
      "url": "${BOT_ACTIVITY_URL}"
    }
  }
}
